@{
    Layout = null;
}

@model WBK.Models.Create.PageQuestionModel

<div class="profileQuestion questionDiv">
    <h3 style="float: left;">Vraag @(Model.QuestionId + 1)</h3>
    <div style="float: right;" class="dropleft show">
        <a class="btn btn-secondary dropdown-toggle" href="#" role="button" id="dropdownMenu_Page@(Model.PageId)_Question@(Model.QuestionId)" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            =
        </a>
        <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
            <a style="color: red;" href="" class="profileQuestion removeQuestionClass">Vraag verwijderen</a>
        </div>
    </div>
    <input class="form-control" readonly="readonly" id="PagesList_@(Model.PageId)__Questions_@(Model.QuestionId)__Title " name="PagesList[@(Model.PageId)].Questions[@(Model.QuestionId)].Title" type="text" value="Wat is uw postcode?" />
    <input class="form-control" readonly="readonly" id="PagesList_@(Model.PageId)__Questions_@(Model.QuestionId)__Attribute " name="PagesList[@(Model.PageId)].Questions[@(Model.QuestionId)].Attribute" type="text" value="postcode" hidden />

    <select class="form-control" readonly="readonly" data-val="true" data-val-required="The Type field is required." id="PagesList_@(Model.PageId)__Questions_@(Model.QuestionId)__Type" name="PagesList[@(Model.PageId)].Questions[@(Model.QuestionId)].Type">
        <option value="0">GeoVraag</option>
        <option value="1">MeerkeuzeVraag</option>
        <option selected="selected" value="2">OpenVraag</option>
        <option value="3">SliderVraag</option>
        <option value="4">NummerVraag</option>
    </select>

    <select class="form-control" readonly="readonly" data-val="true" data-val-required="The Category field is required." id="PagesList_@(Model.PageId)__Questions_@(Model.QuestionId)__Category" name="PagesList[@(Model.PageId)].Questions[@(Model.QuestionId)].Category">
        <option selected="selected" value="0">Profiel</option>
        <option value="1">Locatie</option>
        <option value="2">Gezondheid</option>
    </select>
    <label for="QuestionCounter" style="display: none;">@(Model.QuestionId)</label>

</div>
